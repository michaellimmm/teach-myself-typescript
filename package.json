{
  "name": "teach-myself-typescript",
  "version": "1.0.0",
  "description": "teach myself typescript",
  "main": ".dist/index.js",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^29.2.5",
    "@typescript-eslint/eslint-plugin": "^5.48.1",
    "@typescript-eslint/parser": "^5.48.1",
    "eslint": "^8.32.0",
    "jest": "^29.3.1",
    "prettier": "2.8.3",
    "ts-jest": "^29.0.5",
    "typescript": "^4.9.4"
  },
  "scripts": {
    "test": "./node_modules/.bin/jest ./tests",
    "lint:check": "./node_modules/.bin/eslint -c ./.eslintrc.cjs \"./{src, tests}/*.ts\"",
    "lint:fix": "./node_modules/.bin/eslint -c ./.eslintrc.cjs \"./{src, tests}/*.ts\" --fix",
    "prettier:check": "./node_modules/.bin/prettier --check \"./{src, tests}/*.ts\"",
    "prettier:write": "./node_modules/.bin/prettier --write \"./{src, tests}/*.ts\""
  }
}
